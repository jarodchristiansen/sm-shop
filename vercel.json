{
    "headers": [
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "DENY"
          },
          {
            "key" : "X-XSS-Protection",
            "value" : "1; mode=block"
          },
          {
            "key" : "Referrer-Policy",
            "value" : "strict-origin"
          },
          {
            "key" : "Permissions-Policy",
            "value" : "geolocation=(self), microphone=()"
          },
          {
            "key": "Content-Security-Policy",
            "value":
            "style-src 'self' *.gstatic.com *.googleapis.com https://*.icomoon.io; script-src 'self' 'nonce-rAnd0m123' *.googletagmanager.com; object-src 'none'; base-uri 'none'; require-trusted-types-for 'script';"
          }
        ]
      }
    ]
  }